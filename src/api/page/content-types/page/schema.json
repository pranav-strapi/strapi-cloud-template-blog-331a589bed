    },
    "banner": {
      "type": "component",
      "repeatable": false,
      "component": "shared.page-banner"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.text-editor",
        "shared.main-page-overview",
        "shared.image",
        "stream.stream-card-grid",
        "stream.company-policy",
        "page.google-sheet",
        "holiday-calendar.holiday-calendar-list"
      ]
    },
    "showDownloadButton": {
      "type": "boolean"
    },
    "sortingSelector": {
      "type": "component",
      "repeatable": false,
      "component": "page.sorting-option-selector"
    }
  }
